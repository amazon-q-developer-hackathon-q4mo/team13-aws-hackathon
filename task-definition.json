{
  "family": "LiveInsight-task",
  "taskRoleArn": "arn:aws:iam::730335341740:role/LiveInsight-ecs-task",
  "executionRoleArn": "arn:aws:iam::730335341740:role/LiveInsight-ecs-task-execution",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "LiveInsight-container",
      "image": "730335341740.dkr.ecr.us-east-1.amazonaws.com/liveinsight-app:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "AWS_DEFAULT_REGION",
          "value": "us-east-1"
        },
        {
          "name": "DEBUG",
          "value": "False"
        },
        {
          "name": "EVENTS_TABLE",
          "value": "LiveInsight-Events"
        },
        {
          "name": "SESSIONS_TABLE",
          "value": "LiveInsight-Sessions"
        },
        {
          "name": "ACTIVE_SESSIONS_TABLE",
          "value": "LiveInsight-ActiveSessions"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/LiveInsight",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8000/health/ || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}